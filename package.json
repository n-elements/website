    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "docusaurus-lunr-search": "2.0.1",
    "file-loader": "6.0.0",
    "postcss-easy-import": "3.0.0",
    "postcss-import-url": "5.1.0",
    "postcss-inset": "1.0.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-portal-frame": "1.0.6",
